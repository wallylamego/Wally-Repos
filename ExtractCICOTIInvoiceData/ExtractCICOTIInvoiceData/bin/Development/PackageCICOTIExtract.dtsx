              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; Â© Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="SQL Server Source"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
T1. Reference
,T1. TxDate
,T1. PostST_dCreatedDate AS 'Created Date'
,T2. cAgentName AS 'Agent Name'
,T1.WarehouseID
,T3.Code AS 'Item Code'
,T3.Description_1 AS 'Item Description'
,T4. Account  AS 'Customer Account'
,T4. Name AS 'Customer Name'
,T1.Quantity
,T1. Credit - Debit AS 'Amount'
FROM [Cicoti Maputo].[dbo].[PostST] T1
             Inner Join _rtblAgents T2 on T1.PostST_iCreatedAgentID = T2.idAgents
             Inner Join stkitem T3 on T1.accountlink = T3.stocklink
             inner join Client T4 on T1.DrCrAccount = T4.DCLink          
WHERE ID IN ('oINV','CRN')
AND TxDate &gt; DATEADD(d,-10,GetDate()) AND WarehouseID = 2
order by Reference</property>
